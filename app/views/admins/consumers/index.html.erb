<div class="container">
	<div class="row">
		<div class="col-md-10">
			<div class="outer">
				<table class="table table-striped">
					<% @consumers.each do |consumer| %>
						<thead  class="thead-dark">
							<tr>
								<th>名前</th>
								<th>性別</th>
								<th>電話番号</th>
								<th>年齢</th>
								<th>ステータス</th>
							</tr>
						</thead>
						<tbody>
							<tr>
								<td>
									<%=link_to(admins_consumer_path(consumer)) do %>
										<%= consumer.name %>
									<% end %>
								</td>
								<td><%= consumer.gender_i18n %></td>
								<td><%= consumer.phone %></td>
								<td><%= consumer.age_i18n %></td>
								<td><%= consumer.withdraw_i18n %></td>
							</tr>
						</tbody>
					<% end %>
				</table>
			</div>
		</div>
	</div>
</div>
